{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "alperenersoy/filament-export": "^3.0",
        "altwaireb/laravel-world": "^1.0",
        "ariaieboy/filament-currency": "^1.8",
        "aymanalhattami/filament-page-with-sidebar": "^2.5",
        "bezhansalleh/filament-language-switch": "^3.1",
        "bezhansalleh/filament-shield": "^3.3",
        "filament/filament": "^3.3",
        "filament/forms": "^3.2",
        "flowframe/laravel-trend": "^0.4.0",
        "google/apiclient": "*",
        "guava/filament-nested-resources": "^1.2",
        "hasnayeen/themes": "^3.0",
        "konnco/filament-import": "^3.0",
        "laravel/framework": "^11.9",
        "laravel/reverb": "^1.0",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.5",
        "pelmered/filament-money-field": "^1.4",
        "pusher/pusher-php-server": "^7.2",
        "rmsramos/activitylog": "^1.0",
        "shuvroroy/filament-spatie-laravel-health": "^2.3",
        "smalot/pdfparser": "^2.0",
        "spatie/cpu-load-health-check": "^1.0",
        "spatie/laravel-backup": "^9.3",
        "spatie/security-advisories-health-check": "^1.3",
        "ysfkaya/filament-phone-input": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php -r \"file_exists('bootstrap/cache/services.php') && unlink('bootstrap/cache/services.php');\"",
            "@php -r \"file_exists('bootstrap/cache/packages.php') && unlink('bootstrap/cache/packages.php');\"",
            "@php -r \"\\$dir = 'bootstrap/cache/filament'; if (is_dir(\\$dir)) { \\$files = new \\RecursiveIteratorIterator(new \\RecursiveDirectoryIterator(\\$dir, \\RecursiveDirectoryIterator::SKIP_DOTS), \\RecursiveIteratorIterator::CHILD_FIRST); foreach (\\$files as \\$fileinfo) { \\$todo = (\\$fileinfo->isDir() ? 'rmdir' : 'unlink'); @\\$todo(\\$fileinfo->getRealPath()); } @rmdir(\\$dir); }\"",
            "@php artisan config:clear",
            "@php artisan clear-compiled",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
